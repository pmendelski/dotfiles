#!/bin/bash
# ~/.bash_profile: executed by bash for login shells.

# Login Shell Startup Files:
# 1. /etc/profile
# 2. ~/.bash_profile OR ~/.bash_login OR ~/.profile
# 3. ~/.bash_logout

[[ -s "$HOME/.profile" ]] && source "$HOME/.profile"
# ~/.bashrc and /etc/bash.bashrc are sourced from ~/.profile
